      "body_schema": {
        "name": "string",
        "type": { "type": "ChannelType", "optional": true },
        "topic": { "type": "string", "optional": true },
        "bitrate": { "type": "number", "optional": true },
        "user_limit": { "type": "number", "optional": true },
